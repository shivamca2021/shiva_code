<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data>
		<template id="assets_backend" inherit_id="web.assets_backend" name="product_configured assets backend">
            <xpath expr="script[last()]" position="after">
                <script type="text/javascript" src="/product_configured/static/src/js/product_configuator.js"></script>
            </xpath>
            <xpath expr="link[last()]" position="after">
                <link rel="stylesheet" type="text/scss" href="/product_configured/static/src/scss/product_config.scss"/>
            </xpath>
        </template>
    </data>
</odoo>
